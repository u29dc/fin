{
	"name": "@fin/web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "bunx --bun vite dev",
		"build": "bunx --bun vite build",
		"preview": "bunx --bun vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "bunx --bun svelte-kit sync && bunx --bun tsgo --noEmit && bunx --bun svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"dependencies": {
		"@lucide/svelte": "^0.561.0",
		"@fin/core": "workspace:*",
		"echarts": "^6.0.0"
	},
	"devDependencies": {
		"bun-types": "^1.3.5",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.18",
		"svelte": "^5.43.8",
		"svelte-adapter-bun": "^1.0.1",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.2"
	}
}
